{
  "endpoints": [
    {
      "endpoint": "/servers/{sid}/api/create_channel",
      "id": "api-create-server-channel",
      "method": "post",
      "description": "Creates a new channel on the specified server.",
      "response": [
        "{",
        "    \"token\": \"new_token\"",
        "}"
      ],
      "request": [
        "curl -X POST 'https://onlinedi.vision/servers/1/api/create_channel' \\",
        "-H 'Content-Type: application/json' \\",
        "-d '{",
        "    \"token\": \"valid_token\",",
        "    \"channel_name\": \"info\",",
        "    \"username\": \"user123\"",
        "}'"
      ],
      "parameters": [
        {
          "name": "token",
          "type": "string",
          "required": "Yes",
          "description": "User's authentication token."
        },
        {
          "name": "username",
          "type": "string",
          "required": "Yes",
          "description": "Username of the user creating the channel."
        },
        {
          "name": "channel_name",
          "type": "string",
          "required": "Yes",
          "description": "Name of the new channel to be created."
        },
        {
          "name": "sid",
          "type": "string",
          "required": "Yes",
          "description": "Server ID where the channel will be created."
        }
      ]
    }
  ]
}
