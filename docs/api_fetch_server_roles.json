{
  "endpoints": [
    {
      "endpoint":"/api/fetch_server_roles",
      "id":"api-fetch-server-roles",
      "method":"get",
      "description":"Retrieves the list of all roles defined for a given server.",
      "response": [
        "{",
        "   \"role_name\": \"Moderator\", ",
        "   \"server_id\": \"valid_server_id\", ",
        "   \"color\": \"#FF0000\", ",
        "   \"permissions\": \"['kick_users', 'ban_users']\"",
        "}",
        "{",
        "   \"role_name\": \"Admin\", ",
        "   \"server_id\": \"valid_server_id\", ",
        "   \"color\": \"#FFFFFF\", ",
        "   \"permissions\": \"['add_users', 'kick_users', 'ban_users', 'delete_server']\"",
        "}"
      ],
      "request": [
        "curl -X GET 'https://onlinedi.vision/api/fetch_server_roles?token=valid_token&username=username123&server_id=valid_server_id' "
      ],
      "parameters": [
        {
          "name":"token",
          "type":"string",
          "required":"Yes",
          "description":"User authentication token."
        },

        {
          "name":"username",
          "type":"string",
          "required":"Yes",
          "description":"User's unique username."
        },

        {
          "name":"server_id",
          "type":"string",
          "required":"Yes",
          "description":"ID of server to list all roles from."
        }
      ]
    }
  ]
}
